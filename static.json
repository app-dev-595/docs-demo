{
  "root": "www/",
  "clean_urls": true,
  "https_only": true,
  "routes": {
    "/assets/*": "/assets/",
    "/build/*": "/build/",
    "/**": "index.html"
  },
  "headers": {
    "/build/p-*.js": {
      "Cache-Control": "public, max-age=86400"
    },
    "/**": {
      "Cache-Control": "public, max-age=600"
    }
  }
}
